{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/main.ts"],
  "project": ["src/**/*.vue", "src/**/*.ts"],
  "ignore": [
    "node_modules/**",
    "dist/**",
    "wailsjs/**",
    "dist-electron/**",
    "**/*.spec.ts",
    "**/*.test.ts",
    "cypress/**",
    "vitest.config.ts",
    "vite.config.ts"
  ],
  "ignoreDependencies": ["@cypress/*"],
  "ignoreIssues": {
    "src/composables/article/useContentTranslation.ts": ["exports"],
    "src/utils/clipboard.ts": ["exports"],
    "src/utils/fontDetector.ts": ["exports"],
    "src/utils/mediaProxy.ts": ["exports"],
    "src/components/settings/index.ts": ["exports"],
    "src/stores/app.ts": ["types"],
    "src/types/models.ts": ["types"],
    "src/config/defaults.ts": ["types"],
    "src/types/discovery.ts": ["types"],
    "src/types/settings.ts": ["types"],
    "src/components/settings/advanced/KeyValueList.vue": ["types"]
  }
}
